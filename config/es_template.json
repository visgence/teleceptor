{
    "template": "teleceptor-*",
    "mappings": {
        "teledata": {
            "properties": {
                "@timestamp": {
                    "type": "date",
                    "format": "epoch_millis"
                },

                "ds": {
                    "type": "long"
                },
                "value": {
                    "type": "double"
                }
            }
        }
    }
}

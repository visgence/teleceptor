{
  "author": "Visgence Inc.",
  "buildDate": 1524768088.252515,
  "dependencies": {
    "@uirouter/angularjs": "1.0.15",
    "adm-dtp": "1.2.0",
    "angular": "1.6.10",
    "angular-animate": "1.6.10",
    "angular-aria": "1.6.10",
    "angular-material": "1.1.8",
    "angular-route": "1.6.10",
    "d3": "5.1.0",
    "jquery": "3.3.1",
    "jstree": "3.3.5"
  },
  "description": "Teleceptor is an open-source data logger and feature-rich dashboard.",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-angularjs-annotate": "0.8.2",
    "babel-preset-env": "1.6.1",
    "concurrently": "3.5.1",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.11.0",
    "exports-loader": "0.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "htmlhint": "0.9.13",
    "imports-loader": "0.8.0",
    "less": "3.0.2",
    "node-sass": "4.8.3",
    "sass-lint": "1.12.1",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "3.11.0",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-bundle-tracker": "0.3.0"
  },
  "name": "Teleceptor",
  "private": "true",
  "scripts": {
    "build": "npm run clean && npm run watch",
    "buildProduction": "npm run clean && npm run webpackProduction",
    "checkOutdated": "npm outdated && pip list --outdated",
    "clean": "rm -fr static/dist/",
    "eslint": "./node_modules/.bin/eslint src",
    "htmllint": "./node_modules/.bin/htmlhint --config .htmlhintrc src",
    "incMajor": "./teleceptorcmd majorVersion", 
    "incMinor": "./teleceptorcmd minorVersion",
    "installGitHooks": "printf '%s#!/bin/bash %s\n %s\necho \"Running tests.\" %s\nnpm run tests' > .git/hooks/pre-push && chmod +x .git/hooks/pre-push",
    "lint": "npm run htmllint && npm run scsslint && npm run eslint && npm run pythonlint",
    "postinstall": "npm run installGitHooks",
    "pythonlint": "pep8 --exclude='*node_modules*' . --max-line-length=200",
    "scsslint": "./node_modules/.bin/sass-lint src '**/*.scss' -v -q",
    "setupTeleceptor": "./teleceptorcmd setup",
    "start": "npm run build & concurrently --kill-others './teleceptorcmd runserver'",
    "startProduction": "npm run buildProduction & concurrently --kill-others './teleceptorcmd runserver'",
    "test": "python unittests/unitTests.py && python2 unittests/apiTests.py",
    "tests": "npm run test && npm run lint",
    "watch": "webpack --watch --progress --config ./webpack.development.config.js",
    "webpack": "webpack --progress --config ./webpack.development.config.js",
    "webpackProduction": "webpack --progress --config ./webpack.production.config.js"
  },
  "version": "1.9.3"
}
